  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for iGov
  pod 'Alamofire', '~> 3.5' #, :git => 'https://github.com/Alamofire/Alamofire.git', :branch => 'swift2.3'
  pod 'SwiftyJSON'
  pod 'MagicalRecord'
end

